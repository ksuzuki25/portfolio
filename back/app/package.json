{
  "name": "todo-app",
  "version": "0.0.1",
  "description": "todo-app with serverless architecture",
  "main": "src/app.ts",
  "author": "SIG",
  "private": true,
  "scripts": {
    "build": "tsc && tsc-alias && \\cp -f .env ../dist/function && \\cp -rf ../dist/function/. ../.aws-sam/build/ApiFunction/",
    "build:watch": "tsc-watch --onSuccess \"sh -c 'tsc-alias && \\cp -f .env ../dist/function && \\cp -rf ../dist/function/. ../.aws-sam/build/ApiFunction/'\"",
    "build:deploy": "rm -rf ../dist && tsc --project ../sam-tsconfig.json && tsc-alias -p ../sam-tsconfig.json && synp --source-file ./yarn.lock && mkdir -p ../dist/layer && cp ./package*.json ../dist/layer/ && rm -rf ./package-lock.json",
    "lint": "run-p -l -c --aggregate-output lint:*",
    "lint:eslint": "eslint .",
    "lint:prettier": "prettier --check .",
    "fix": "run-s fix:prettier fix:eslint",
    "fix:eslint": "npm run lint:eslint -- --fix",
    "fix:prettier": "npm run lint:prettier -- --write",
    "test": "jest",
    "test:debug": "jest --silent=false --verbose false",
    "test:watch": "jest --watchAll",
    "test:coverage": "jest --coverage",
    "test:clear": "jest --clearCache"
  },
  "dependencies": {
    "@vendia/serverless-express": "^4.5.3",
    "ajv": "^8.9.0",
    "ajv-errors": "^3.0.0",
    "ajv-formats": "^2.1.1",
    "axios": "^0.25.0",
    "cookie-parser": "^1.4.6",
    "dotenv": "^14.3.2",
    "dynamoose": "^2.8.5",
    "express": "^4.17.2",
    "helmet": "^5.0.2",
    "jose": "^4.8.1"
  },
  "devDependencies": {
    "@aws-sdk/client-dynamodb": "^3.51.0",
    "@aws-sdk/lib-dynamodb": "^3.51.0",
    "@jest-mock/express": "^1.4.5",
    "@shelf/jest-dynamodb": "^2.2.3",
    "@types/aws-lambda": "^8.10.92",
    "@types/cookie-parser": "^1.4.2",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/jest": "^27.4.0",
    "@types/node": "14",
    "@types/request-promise": "^4.1.46",
    "@types/supertest": "^2.0.11",
    "@types/uuid": "^8.3.4",
    "@typescript-eslint/eslint-plugin": "^5.10.2",
    "@typescript-eslint/parser": "^5.10.2",
    "esbuild": "^0.14.21",
    "esbuild-jest": "^0.5.0",
    "eslint": "^8.7.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-jest": "^26.0.0",
    "jest": "^27.4.7",
    "jest-html-reporter": "^3.4.2",
    "nock": "^13.2.4",
    "prettier": "2.5.1",
    "source-map-support": "^0.5.21",
    "supertest": "^6.2.2",
    "synp": "^1.9.9",
    "tsc-alias": "^1.5.0",
    "tsc-watch": "^4.6.0",
    "tslint": "^6.1.1",
    "typescript": "^4.5.4",
    "yarn-run-all": "^3.1.1"
  }
}
